<%= form_for(user) do |f| %>

    <div class="field">
        <%= f.label :name%>
        <%= f.text_field :name%>
    </div>

    <div class="field">
        <%= f.label :Email%>
        <%= f.text_field :email%>
    </div>

    <div class="field">
        <%= f.label :Age%>
        <%= f.text_field :age%>
    </div>

    <div class="field">
        <%= f.label :password%>
        <%= f.password_field :password%>
    </div>
    

    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>